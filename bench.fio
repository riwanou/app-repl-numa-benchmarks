[global]
group_reporting
ioengine=mmap
size=128m
numjobs=$ncpus
thread=1
directory=/tmp
filename=/tmp/fio_readwrite
time_based
runtime=${RUNTIME}
share_mmap=1
repl=1

[writers]
rw=randwrite
random_distribution=${DISTRIB}
numjobs=${WRITEJOBS}

[readers]
rw=randread
random_distribution=${DISTRIB}
numjobs=${READJOBS}
